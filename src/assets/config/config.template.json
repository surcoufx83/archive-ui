{
  "api": {
    "baseUrl": "/api"
  },
  "auth": {
    "authUrl": "/api/auth/login",
    "authCheck": "/api/auth/check",
    "basic": {
      "enabled": false,
      "user": "",
      "password": ""
    },
    "oauth2": {
      "enabled": true,
      "items": {
        "<your server>": {
          "endpoint": "https://<your nextcloud server>/apps/oauth2/authorize",
          "tokenEndpoint": "/api/auth/oauth2",
          "clientId": "<your client id>",
          "clientSecret": "<your client secret>",
          "redirectUrl": "https://<your server>/login/oauth2",
          "state": "<your server>"
        },
        "localhost:4200": {
          "endpoint": "https://<your nextcloud server>/apps/oauth2/authorize",
          "tokenEndpoint": "/api/auth/oauth2",
          "clientId": "<your client id>",
          "clientSecret": "<your client secret>",
          "redirectUrl": "https://localhost:4200/login/oauth2",
          "state": "localhost:4200"
        }
      }
    }
  },
  "navbar": {
    "items": [
      { "title": "navbar.items.home", "icon": "home", "link": "/home" },
      { "title": "navbar.items.dashboard", "icon": "dashboard", "link": "/dashboard" },
      { "title": "navbar.items.cases", "icon": "cases", "link": "/cases" },
      { "title": "navbar.items.finance", "icon": "finance", "link": "/finance" },
      { "title": "navbar.items.notepad", "icon": "notepad", "link": "/notepad" },
      { "title": "navbar.items.work", "icon": "work", "link": "/work" },
      { "title": "navbar.items.account", "icon": "account", "link": "/account" }
    ],
    "workitems": [
      { "title": "navbar.workitems.today", "icon": "today", "link": "/work/today" },
      { "title": "navbar.workitems.month", "icon": "calendar", "link": "/work/month" },
      { "title": "navbar.workitems.leads", "icon": "leads", "link": "/work/leads" },
      { "title": "navbar.workitems.settings", "icon": "settings", "link": "/work/settings" }
    ]
  },
  "storage": {
    "prefix": "ArcApiv2__"
  }
}
