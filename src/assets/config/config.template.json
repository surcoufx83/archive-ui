    "authUrl": "/api/auth/login",
    "authCheck": "/api/auth/check",
    "basic": {
      "enabled": false,
      "user": "",
      "password": ""
    },
    "oauth2": {
      "enabled": true,
      "endpoint": "https://<your nextcloud server>/apps/oauth2/authorize",
